SELECT COUNT(*) FROM  ( SELECT COUNT(*) groups FROM students GROUP BY number_aunts,fname,zipcode) temp WHERE temp.groups <= 10000;
SELECT COUNT(*) FROM  ( SELECT COUNT(*) groups FROM students GROUP BY state,fname) temp WHERE temp.groups <= 10000;

